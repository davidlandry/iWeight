<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controllers.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>controllers.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddUser</servlet-name>
        <servlet-class>controllers.AddUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddLocation</servlet-name>
        <servlet-class>controllers.AddLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddExerciseType</servlet-name>
        <servlet-class>controllers.AddExerciseType</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddExercise</servlet-name>
        <servlet-class>controllers.AddExercise</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddWorkout</servlet-name>
        <servlet-class>controllers.AddWorkout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeAbout</servlet-name>
        <servlet-class>controllers.ChangeAbout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePassword</servlet-name>
        <servlet-class>controllers.ChangePassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeEmail</servlet-name>
        <servlet-class>controllers.ChangeEmail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddLocation</servlet-name>
        <url-pattern>/AddLocation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddUser</servlet-name>
        <url-pattern>/AddUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddExerciseType</servlet-name>
        <url-pattern>/AddExerciseType</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddExercise</servlet-name>
        <url-pattern>/AddExercise</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddWorkout</servlet-name>
        <url-pattern>/AddWorkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeAbout</servlet-name>
        <url-pattern>/ChangeAbout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePassword</servlet-name>
        <url-pattern>/ChangePassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeEmail</servlet-name>
        <url-pattern>/ChangeEmail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>404</error-code>
        <location>/ErrorPages/Error404.html</location>
    </error-page>
</web-app>
